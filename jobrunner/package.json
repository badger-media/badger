{
  "name": "bowser-jobrunner",
  "version": "0.4.3",
  "packageManager": "yarn@3.6.1",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "esbuild": "^0.18.16",
    "eslint": "latest",
    "prettier": "^3.0.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "type": "commonjs",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "prettify": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "build": "esbuild --bundle src/index.ts --outfile=dist/index.cjs --sourcemap --platform=node --target=node18 --define:__APP_VERSION__=\\\"$npm_package_version\\\" --define:__GIT_COMMIT__=\\\"$(git rev-parse HEAD)\\\"",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.374.0",
    "@bowser/prisma": "workspace:*",
    "@bowser/testing": "workspace:*",
    "@googleapis/drive": "^8.0.0",
    "@sentry/node": "^7.64.0",
    "@types/dotenv-flow": "^3.2.0",
    "dotenv-flow": "^3.2.0",
    "got": "^13.0.0",
    "loglevel": "^1.8.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "p-event": "^6.0.0"
  }
}
