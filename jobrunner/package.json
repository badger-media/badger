{
  "name": "bowser-jobrunner",
  "packageManager": "yarn@3.6.1",
  "devDependencies": {
    "@prisma/client": "^5.0.0",
    "@tsconfig/node18": "^18.2.0",
    "esbuild": "^0.18.16",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "tsx": "^3.12.7",
    "zod-prisma-types": "^2.7.4"
  },
  "type": "module",
  "scripts": {
    "prisma:generate": "prisma generate --schema=../server/prisma/schema.prisma",
    "dev": "tsx watch src/index.ts",
    "prettify": "prettier --write .",
    "build": "SKIP_ZOD_PRISMA=true yarn prisma:generate && esbuild --bundle src/index.ts --outdir=dist --sourcemap --external:.prisma/client --external:@prisma/client --platform=node --target=node18"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.374.0",
    "@googleapis/drive": "^8.0.0",
    "@types/got": "^9.6.12",
    "got": "^13.0.0",
    "loglevel": "^1.8.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "p-event": "^6.0.0"
  }
}
